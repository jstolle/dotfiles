[user]
	name = Josh Tolle (jstolle)
	email = josh@jstolle.com
[color]
        ui = auto
[alias]
	br = branch
	ci = commit
	co = checkout
	cp = cherry-pick
	df = diff
        dfc = diff --cached
	fp = format-patch
	lg = log -p
	st = status
[core]
	excludesfile = /Users/jostol01/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/jostol01/.stCommitMsg
